{
  "id": "/jenkins-deployed-app",
  "cmd": "jekyll serve",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "aamir010/marathon-demo:latest"
    },
    "portMappings": [
      {
        "containerPort": 4000,
        "hostPort": 0,
        "protocol": "tcp",
        "name": "jenkins-deployed-app"
      }
    ]
  },
  "cpus": 0.1,
  "instances": 1,
  "mem": 128,
  "networks": [
    {
      "mode": "container/bridge",
      "labels": {}
    }
  ],
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  }
}
